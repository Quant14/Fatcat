public Person createPerson(Person person) {
        AES256 aes = new AES256();
        person.setName(aes.encrypt(person.getName()));
        person.setSector(aes.encrypt(person.getSector()));
        person.setDescription(aes.encrypt(person.getDescription()));
        person.setDanger(aes.encrypt(person.getDanger()));
        return fatcatServerRepository.save(person);
}

public List<Person> getPerson() {
    AES256 aes = new AES256();
    List<Person> tempList = fatcatServerRepository.findAll();
    for(int i = 0; i < tempList.size(); i++) {
        tempList.get(i).setName(aes.decrypt(tempList.get(i).getName()));
        tempList.get(i).setSector(aes.decrypt(tempList.get(i).getSector()));
        tempList.get(i).setDescription(aes.decrypt(tempList.get(i).getDescription()));
        tempList.get(i).setDanger(aes.decrypt(tempList.get(i).getDanger()));
   }
   return tempList;
   }
